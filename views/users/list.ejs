<%- include ../partials/head%>
<%- include ../partials/menu%>


<div class="container">
  <br>          
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Username</th>
        <th>Password</th>
        <th>Firstname</th>
        <th>Lastname</th>
        <th>Email</th>
        <th>No. Telp</th>
        <th>Level</th>
      
      </tr>
    </thead>
    <tbody>
      <%users.forEach( user => {%>
        <tr>
          <td><%=user.username%></td>
          <td><%=user.password%></td>
          <td><%=user.first_name%></td>
          <td><%=user.last_name%></td>
          <td><%=user.email%></td>
          <td><%=user.no_telp%></td>
          <td><%=user.level%></td>
        </tr>
      <%})%>
      
    </tbody>
  </table>
</div>

<%- include ../partials/footer%>

