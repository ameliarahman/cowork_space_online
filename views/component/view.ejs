<% include ../partials/head%>
	<% include ../partials/basicmenu%>
		<% include ../partials/menu%>
		

			<div id="wrapper">
				<section class="main items">
					<% dataRooms.forEach((room)=>{ %>
						<article class="item">
							<header>
								<a href="/booking/details/<%= room.id%>">
									<img src="/images/<%=room.photo_url%>" alt="<%= room.name%>">
								</a>
								<h3>
									<%= room.name%>
								</h3>
							</header>
							<h5>
								<%= room.address%>,
									<%= room.city%>
							</h5>
							<ul class="actions">
								<li><a href="/booking/details/<%= room.id%>" class="button">More</a></li>
							</ul>
						</article>
						<%})%>
				</section>

