<form action="/booking/details/<%=dataRoom%>" method="POST">
  <div class="form-group">
    <label>Firstname:</label>
    <input type="text" class="form-control" name="first_name" value="<%=user.username%>">
  </div>
  <div class="form-group">
    <label>Lastname:</label>
    <input type="text" class="form-control" name="last_name" value="<%=user.last_name%>">
  </div>
  <div class="form-group">
    <label>Email:</label>
    <input type="text" class="form-control" name="email" value="<%=user.email%>">
  </div>
  <div class="form-group">
    <label>No.Telp:</label>
    <input type="text" class="form-control" name="no_telp" value="<%=user.no_telp%>">
  </div>
  <div class="form-group">
    <label>Reserved Days</label>
    <input type="text" class="form-control" name="date" value="<%=user.count_booking_date%>">
  </div>
  <div class="form-group">
    <label>From Date</label>
    <input type="text" class="form-control" name="from_date" value="<%=user.from_date%>">
  </div>
  <div class="form-group">
    <label>To Date</label>
    <input type="text" class="form-control" name="to_date" value="<%=user.to_date%>">
  </div>
  <div class="form-group">
    <label>Total Price</label>
    <input type="text" class="form-control" name="price_total" value="<%=user.price%>">
  </div>
  
  <button type="submit" class="btn btn-default">Submit</button>
</form>